#include <stdio.h>


int main()
{
   
   for (int i = 1; i <= 10; i++)
   {
       int j;

       for (j = 10; j >= i; j--)
       {
           printf(" ");
       
       }
       
       for (j = 1; j <= i; j++) 
       {

           printf("*");

       }
       
       for (int k = i-1; k >= 1; k--)
       {

           printf("*");

       }
       
     
       printf("\n");
       

   }

  return 0;
}
